model {
  #hyperparameters
  p_mu <- 0
  p_tau <- 0.01
  
  #priors
  intercept ~ dnorm(p_mu, p_tau)
  beta_rep ~ dnorm(p_mu, p_tau)
  beta_w1 ~ dnorm(p_mu, p_tau)
  beta_w2 ~ dnorm(p_mu, p_tau)
  
  #likelihood
  for(i in 1:n){
    log(mu[i]) <- intercept + beta_rep * rep[i] + beta_w1 * w1[i] + beta_w2 * w2[i]
    death[i] ~ dpois(mu[i])
  }
}